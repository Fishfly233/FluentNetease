<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FluentNetease">
    
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="AliasTextBrush" Color="Gray"/>
        </ResourceDictionary>
            
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="AliasTextBrush" Color="LightGray"/>
        </ResourceDictionary>
            
        <ResourceDictionary x:Key="HighContrast">
            <SolidColorBrush x:Key="AliasTextBrush" Color="Yellow"/>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
